<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />

	<action execute="#{notastakeholderHome.wire}" />

	<param name="notastakeholderFrom" />
	<param name="notastakeholderId" value="#{notastakeholderHome.notastakeholderId}" />
	<param name="notaFrom" />
	<param name="notaId" value="#{notaHome.notaId}" />

	<param name="projetoFrom" />
	<param name="projetoId" value="#{projetoHome.projetoId}" />
	<param name="statusprojetoFrom" />
	<param name="statusprojetoId" value="#{statusprojetoHome.statusprojetoId}" />

	<param name="usuarioFrom" />
	<param name="usuarioId" value="#{usuarioHome.usuarioId}" />


	<param name="stakeholderFrom" />
	<param name="stakeholderId" value="#{stakeholderHome.stakeholderId}" />



	<navigation from-action="#{avaliacao.salvarNotaStakeholder()}">
		<rule if-outcome="salvou">
			<redirect view-id="/view/notastakeholder/NotastakeholderEdit.xhtml">
				<param name="projetoId" value="#{projetoHome.projetoId}" />
			</redirect>
		</rule>
	</navigation>

	<navigation from-action="#{notastakeholderHome.update}">
		<rule if-outcome="updated">
			<end-conversation />
			<redirect view-id="/Notastakeholder.xhtml" />
		</rule>
	</navigation>

	<navigation from-action="#{notastakeholderHome.remove}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/NotastakeholderList.xhtml" />
		</rule>
	</navigation>

</page>
